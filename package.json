{
  "name": "shiki-magic-move",
  "type": "module",
  "version": "1.1.0",
  "packageManager": "pnpm@10.17.1",
  "description": "Smoothly animated code blocks with Shiki",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/shikijs/shiki-magic-move#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shikijs/shiki-magic-move.git"
  },
  "bugs": "https://github.com/shikijs/shiki-magic-move/issues",
  "keywords": [
    "shiki",
    "animations"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./core": "./dist/core.mjs",
    "./renderer": "./dist/renderer.mjs",
    "./types": "./dist/types.mjs",
    "./vue": "./dist/vue.mjs",
    "./react": "./dist/react.mjs",
    "./solid": "./dist/solid.mjs",
    "./svelte": "./dist/svelte.mjs",
    "./style.css": "./dist/style.css",
    "./dist/*": "./dist/*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "./vue": [
        "./dist/vue.d.mts"
      ],
      "./react": [
        "./dist/react.d.mts"
      ],
      "./svelte": [
        "./dist/svelte.d.mts"
      ],
      "./solid": [
        "./dist/solid.d.mts"
      ],
      "./core": [
        "./dist/core.d.mts"
      ],
      "*": [
        "./dist/*",
        "./dist/index.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nr play",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "simple-git-hooks",
    "play": "nr -C playground dev",
    "play:build": "nr -C playground build"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0",
    "shiki": "^1.0.0 || ^2.0.0 || ^3.0.0",
    "solid-js": "^1.9.1",
    "svelte": "^5.0.0-0",
    "vue": "^3.4.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "shiki": {
      "optional": true
    },
    "solid-js": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "dependencies": {
    "diff-match-patch-es": "^1.0.1",
    "ohash": "^2.0.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@antfu/ni": "^26.0.1",
    "@antfu/utils": "^9.2.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint-react/eslint-plugin": "^1.53.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@types/diff-match-patch": "^1.0.36",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@unocss/reset": "^66.5.2",
    "@vueuse/core": "^13.9.0",
    "babel-preset-solid": "^1.9.9",
    "bumpp": "^10.2.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.21",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-svelte": "^3.12.4",
    "lint-staged": "^16.2.0",
    "pnpm": "^10.17.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.2",
    "shiki": "^3.13.0",
    "simple-git-hooks": "^2.13.1",
    "solid-js": "^1.9.9",
    "svelte": "^5.39.5",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1",
    "unocss": "^66.5.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "vue": "^3.5.21",
    "vue-tsc": "^3.0.8"
  },
  "resolutions": {
    "vite": "^7.1.7"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
